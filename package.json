{
  "name": "Procket",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android-bundle": "mkdir -p android/app/src/main/assets && react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone X\"",
    "apk": "npm run bundle && cd android && ./gradlew assembleRelease && cd app/build/outputs/apk && open .",
    "rename": "node ./bin/rename.js",
    "start": "react-native start",
    "test": "jest",
    "preinstall": "chmod 777 ./preinstall.sh && ./preinstall.sh",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/",
    "codepush": "appcenter codepush release-react -a Game-Town/game-town-android -d Production"
  },
  "dependencies": {
